@model IEnumerable<Lab7WebApi.Models.Pacients>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ID_Pacinents)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LastName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.ID_Pacinents)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Name)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.LastName)
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.ID_Pacinents }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.ID_Pacinents }, new { onclick = "return confirm('Видалити ці дані?')" })
    </td>
</tr>
}

</table>
